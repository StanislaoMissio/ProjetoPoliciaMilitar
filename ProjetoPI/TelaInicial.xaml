<Window x:Class="ProjetoPI.TelaInicial"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetoPI"
        mc:Ignorable="d"
        Title="Tela Inicial" Height="500" Width="600" Background="Black">
    <Grid>
        <TabControl x:Name="tabTelaInicial" HorizontalAlignment="Left" Height="453" VerticalAlignment="Top" Width="572" Margin="10,10,0,0">
            <TabItem Header="Cadastrar Boletim de Ocorrência">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Consultar Boletim">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Gerar Relatório">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Cadastrar Usuario" x:Name="tabCadastrarUsuario" Visibility="Hidden" IsEnabled="False">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
